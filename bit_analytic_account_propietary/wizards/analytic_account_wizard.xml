<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
      <record id="analytic_account_wizard_view" model="ir.ui.view">
        <field name="name">Reporte propietario</field>
        <field name="model">analytic.account.wizard</field>
        <field name="arch" type="xml">
          <form>
            <group>
              <field name="date" />
            </group>
            <footer>
              <button type="object" name="button_send" string="Imprimir" />
              <button special="cancel" string="Cancel" class="btn-secondary" />
            </footer>
          </form>
        </field>
      </record>

      <act_window id="action_print_report"
        name="Imprimir Reporte"
        src_model="account.analytic.account"
        res_model="analytic.account.wizard"
        view_mode="form"
        target="new"
        multi="True"
        />
    </data>
</odoo>
