<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="bit_timesheet_custom_timesheet_line_form_view" model="ir.ui.view">
		<field name="name">bit.timesheet.custom.timesheet.line.form.view</field>
        <field name="model">account.analytic.line</field>
        <field name="inherit_id" ref="hr_timesheet.hr_timesheet_line_form" />
